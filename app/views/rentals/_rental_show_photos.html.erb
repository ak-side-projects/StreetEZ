<div class="rental-photos group">
	  <% if rental.photos.first %>
			<%= render "photo_show", file_url: featured_photo.file.url(:big) %>
		
			<div class="photo-thumbs-wrap">
	    	<% rental.photos.each do |photo| %>
					<a href="<%= rental_url(rental.id, photo_id: photo.id) %>">
			      <img class="photo-thumbs" src="<%= photo.file.url(:big) %>" alt="apartment rental photo">
					</a>
		    <% end %>
			</div>
		<% else %>
			<img class="selected-photo" src="<%= asset_path "rental_default.jpeg" %>" alt="apartment rental photo">
	  <% end %>
</div>